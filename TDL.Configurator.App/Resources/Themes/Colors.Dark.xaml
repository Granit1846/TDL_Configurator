<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!-- Dark (default) -->
    <SolidColorBrush x:Key="TDL.Brush.WindowBackground" Color="#FF1E1E1E"/>
    <SolidColorBrush x:Key="TDL.Brush.PanelBackground" Color="#FF252525"/>
    <SolidColorBrush x:Key="TDL.Brush.HeaderBackground" Color="#FF1C1C1C"/>
    <SolidColorBrush x:Key="TDL.Brush.SidebarBackground" Color="#FF202020"/>

    <SolidColorBrush x:Key="TDL.Brush.Text" Color="#FFF0F0F0"/>
    <SolidColorBrush x:Key="TDL.Brush.TextDim" Color="#FFB0B0B0"/>

    <SolidColorBrush x:Key="TDL.Brush.Border" Color="#33FFFFFF"/>
    <SolidColorBrush x:Key="TDL.Brush.ControlBorder" Color="#55FFFFFF"/>

    <SolidColorBrush x:Key="TDL.Brush.ControlBackground" Color="#FF2B2B2B"/>
    <SolidColorBrush x:Key="TDL.Brush.ControlBackgroundHover" Color="#FF303030"/>
    <SolidColorBrush x:Key="TDL.Brush.ControlBackgroundPressed" Color="#FF363636"/>

    <SolidColorBrush x:Key="TDL.Brush.Accent" Color="#FF4FC1FF"/>
    <SolidColorBrush x:Key="TDL.Brush.AccentHover" Color="#FF66CCFF"/>
    <SolidColorBrush x:Key="TDL.Brush.AccentPressed" Color="#FF2AA7F5"/>
    <SolidColorBrush x:Key="TDL.Brush.AccentText" Color="#FF0A0A0A"/>

    <SolidColorBrush x:Key="TDL.Brush.Hover" Color="#22FFFFFF"/>
    <SolidColorBrush x:Key="TDL.Brush.HoverPressed" Color="#33FFFFFF"/>
    <SolidColorBrush x:Key="TDL.Brush.Selection" Color="#224FC1FF"/>

</ResourceDictionary>
